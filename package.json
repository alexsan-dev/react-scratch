{
	"name": "lua-blog",
	"version": "1.0.0",
	"description": "Blog on typescript",
	"main": "index.js",
	"scripts": {
		"babel": "./node_modules/.bin/babel dist --out-dir dist",
		"dev": "webpack-dev-server --open --config webpack.dev.js",
		"webpack": "webpack --config webpack.prod.js",
		"tsc": "node_modules/typescript/bin/tsc",
		"transpiler": "ts-node transpiler.ts",
		"devServer": "ts-node devServer.ts",
		"ssr": "ts-node server.ts",
		"build": "npm run compile --silent && npm run transpiler && npm run ssr && npm run webpack --silent",
		"start": "npm run devServer && npm run dev",
		"compile": "npm run tsc && npm run babel"
	},
	"author": "AlexSantos",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@types/react": "^16.9.44",
		"@types/react-dom": "^16.9.8",
		"awesome-typescript-loader": "^5.2.1",
		"babel-plugin-module-resolver": "^4.0.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^4.2.0",
		"glob": "^7.1.6",
		"html-webpack-plugin": "^4.3.0",
		"mini-css-extract-plugin": "^0.9.0",
		"prettier": "^2.0.5",
		"react-hot-loader": "^4.12.21",
		"source-map-loader": "^1.0.1",
		"style-loader": "^1.2.1",
		"ts-node": "^8.10.2",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.3.0",
		"tslint-react": "^5.0.0",
		"typescript": "^3.9.7",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.0.9"
	},
	"dependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	}
}
