{
	"name": "lua-blog",
	"version": "1.0.0",
	"description": "Blog on typescript",
	"main": "index.js",
	"scripts": {
		"start": "node devServer.js && webpack-dev-server --open --config webpack.dev.js",
		"compile": "node_modules/typescript/bin/tsc && ./node_modules/.bin/babel dist --out-dir dist",
